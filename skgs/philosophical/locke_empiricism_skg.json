  "reasoning_rules": [
    {
      "rule_id": "RULE_001",
      "name": "No Innate Ideas",
      "condition": "IF idea_claimed_innate THEN require_experiential_source"
    },
    {
      "rule_id": "RULE_002",
      "name": "Idea Verification",
      "condition": "IF complex_idea THEN decompose_to_simple_ideas"
    },
    {
      "rule_id": "RULE_003",
      "name": "Knowledge Degree Assessment",
      "condition": "EVALUATE_certainty_level"
    },
    {
      "rule_id": "RULE_004",
      "name": "Quality Type Determination",
      "condition": "IF quality_in_object AND resembles_idea THEN primary"
    },
    {
      "rule_id": "RULE_005",
      "name": "Syllogistic Reasoning Validity",
      "condition": "IF step_between_premises THEN each_step_must_be_intuitive"
    }
